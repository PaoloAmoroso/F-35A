<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

 <switches>
     <battery-switch type="bool">true</battery-switch>
	<landing-lights-switch type="bool">false</landing-lights-switch>
	<nav-lights-switch type="bool">false</nav-lights-switch>
	<beacon-switch type="bool">false</beacon-switch>
	<strobe-switch type="bool">false</strobe-switch>
	<white-light-switch type="bool">false</white-light-switch>	
  </switches>

  <lighting>
    <instruments-norm>0.8</instruments-norm>
    <landing-lights type="bool">false</landing-lights>
    <beacon type="bool">false</beacon>
    <strobe type="bool">false</strobe>
    <nav-lights type="bool">false</nav-lights>
    <cabin-lights type="bool">false</cabin-lights>
    <wing-lights type="bool">false</wing-lights>
    <taxi-light type="bool">false</taxi-light>
    <luminosite-lights type="float">0.8</luminosite-lights>
    <instrument-lights type="bool">false</instrument-lights>
  </lighting>

  <engines>
    <engine n="0">
      <boost type="double">0</boost>
      <magnetos>0</magnetos>
      <noze-flaps-norm type="float">1</noze-flaps-norm>
      <cowl-flaps-norm type="float">1</cowl-flaps-norm>
    </engine>
  </engines>

  <electric>
    <battery-switch>0</battery-switch>
    <generator>0</generator>
    <engines>
      <engine n="0">
        <fuel-pump>0</fuel-pump>
      </engine>
      <engine n="1">
        <fuel-pump>0</fuel-pump>
      </engine>
    </engines>
  </electric>

  <gear>
    <brake-parking type="bool">true</brake-parking>
    <tailwheel-lock type="bool">true</tailwheel-lock>
  </gear>

  <flight>
    <rudder-trim>   0.000 </rudder-trim>
    <aileron-trim>  0.000 </aileron-trim>
    <elevator-trim> 0.080 </elevator-trim>
  </flight>

</PropertyList>
